[MASTER]
ignore=migrations
load-plugins=pylint_django

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    no-member,
    too-many-arguments,
    too-many-locals,
    too-many-instance-attributes,
    too-many-branches,
    too-many-statements,
    duplicate-code,

[FORMAT]
max-line-length=150

[VARIABLES]
good-names=pk,id